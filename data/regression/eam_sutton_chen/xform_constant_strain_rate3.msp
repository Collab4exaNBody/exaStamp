# no-seq

includes:
  - xform_base.cfg

xform_constant_strain_rate:
  mode: triaxial
  strain_rate: 1.0e12
  direction1: [1.0, 0.0, 0.0]
  sign1: 1.0
  direction2: [0.0, 1.0, 0.0]
  sign2: 1.0
  direction3: [0.0, 0.0, 1.0]
  sign3: -1.0

xform_function: xform_constant_strain_rate

simulation_epilog:
  - check_values:
      file: eam_sc_xform_constant_strain_rate3.dat
      threshold: 1.5e-10
  
